@model IEnumerable<Dziennik.Application.Subject.SubjectDto>

<h2>All Subjects</h2>

<table class="table">
    <thead>
        <tr>
            <th>SubjectId</th>
            <th>Name</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var subject in Model)
        {
            <tr>
                <td>@subject.SubjectId</td>
                <td>@subject.Name</td>

            </tr>
        }
    </tbody>
</table>